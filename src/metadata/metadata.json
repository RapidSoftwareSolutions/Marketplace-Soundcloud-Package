        },
        {
          "name": "color",
          "type": "String",
          "info": "The primary color of the widget as a hex triplet. (For example: ff0066).",
          "required": false
        },
        {
          "name": "autoPlay",
          "type": "Boolean",
          "info": "Whether the widget plays on load. Format: true OR false(default).",
          "required": false
        },
        {
          "name": "showComments",
          "type": "Boolean",
          "info": "Whether the player displays timed comments. Format: true(default) OR false.",
          "required": false
        },
        {
          "name": "iframe",
          "type": "Boolean",
          "info": "Whether the new HTML5 Iframe-based Widget or the old Adobe Flash Widget will be returned. Format: true(default) OR false.",
